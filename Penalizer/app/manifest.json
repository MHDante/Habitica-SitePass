{
    "manifest_version": 2,

    "name": "One-click Kittens",
    "description": "This extension demonstrates a browser action with kittens.",
    "version": "1.0",

    "permissions": [
        "storage",
        "activeTab",
        "webRequest",
        "webRequestBlocking",
        "webRequest",
        "<all_urls>",
        "webNavigation"

    ],
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "page": "background.html",
        "persistent": true

  }
}